{
  "name": "reactnativeboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-build": "yarn android-clean; cd android && ./gradlew app:assembleRelease && cd ..",
    "android-build-bundle": "yarn android-clean; cd android && ./gradlew app:bundleRelease && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "pod-install": "npx pod-install ios",
    "ios-clean": "cd ios && rm -rf build && cd ..",
    "android-ios": "react-native run-android; react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "asset-link": "npx react-native-asset"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@reduxjs/toolkit": "^1.8.5",
    "i18next": "^21.9.1",
    "react": "18.0.0",
    "react-i18next": "^11.18.4",
    "react-native": "0.69.4",
    "react-native-flipper": "^0.162.0",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.16.0",
    "react-redux": "^8.0.2",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
