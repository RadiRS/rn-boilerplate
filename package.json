{
  "name": "@radirs/rn-boilerplate",
  "version": "0.0.4",
  "private": true,
  "author": "Radi Rusadi <radi.rusadi@paceprogrammer.com>",
  "description": "RadiRS React Native Boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/RadiRS/rn-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/radirs/rn-boilerplate/issues"
  },
  "scripts": {
    "android": "react-native run-android",
    "android-build": "yarn android-clean; cd android && ./gradlew app:assembleRelease && cd ..",
    "android-build-bundle": "yarn android-clean; cd android && ./gradlew app:bundleRelease && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "pod-install": "npx pod-install ios",
    "ios-clean": "cd ios && rm -rf build && cd ..",
    "android-ios": "react-native run-android; react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "detox-build-ios-debug": "detox build -c ios.debug",
    "detox-build-ios-release": "detox build -c ios.release",
    "detox-test-ios-debug": "detox test -c ios.debug",
    "detox-test-ios-release": "detox test -c ios.release",
    "detox-build-android-debug": "detox build -c android.debug",
    "detox-build-android-release": "detox build -c android.release",
    "detox-test-android-debug": "detox test -c android.debug",
    "detox-test-android-release": "detox test -c android.release",
    "e2e-ios": "yarn detox-build-ios-release && yarn detox-test-ios-release",
    "e2e-android": "yarn detox-build-android-release && yarn detox-test-android-release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "asset-link": "npx react-native-asset",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "np": "np --no-publish",
    "postversion": "react-native-version"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@reduxjs/toolkit": "^1.8.5",
    "i18next": "^21.9.1",
    "react": "18.0.0",
    "react-i18next": "^11.18.4",
    "react-native": "0.69.5",
    "react-native-flipper": "^0.162.0",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-reanimated": "2.10.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-redux": "^8.0.2",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.11",
    "@testing-library/react-native": "^11.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.0.0",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^19.10.0",
    "eslint": "^7.32.0",
    "eslint-plugin-detox": "^1.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.1",
    "metro-react-native-babel-preset": "^0.70.3",
    "np": "^7.6.2",
    "react-dom": "18.0.0",
    "react-native-rename": "^2.9.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  }
}
