{
  "name": "@radirs/rn-boilerplate",
  "version": "0.0.5",
  "private": true,
  "author": "Radi Rusadi <radi.rusadi@paceprogrammer.com>",
  "description": "React Native Boilerplate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RadiRS/rn-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/radirs/rn-boilerplate/issues"
  },
  "scripts": {
    "android": "yarn android-dev-debug",
    "android-dev-debug": "react-native run-android --variant=devdebug --appId=com.rnb.rnboilerplate.dev",
    "android-staging-debug": "react-native run-android --variant=stagdebug --appId=com.rnb.rnboilerplate.stag",
    "android-production-debug": "react-native run-android --variant=proddebug --appId=com.rnb.rnboilerplate",
    "android-dev-release": "react-native run-android --variant=devrelease --appId=com.rnb.rnboilerplate.dev",
    "android-staging-release": "react-native run-android --variant=stagrelease --appId=com.rnb.rnboilerplate.stag",
    "android-production-release": "react-native run-android --variant=prodrelease --appId=com.rnb.rnboilerplate",
    "android-build": "yarn android-clean; cd android && ./gradlew app:assembleRelease && cd ..",
    "android-build-bundle": "yarn android-clean; cd android && ./gradlew app:bundleRelease && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios": "yarn ios-dev-debug",
    "ios-dev-debug": "react-native run-ios --scheme RNBDev --configuration DebugDev",
    "ios-staging-debug": "react-native run-ios --scheme RNBStag --configuration DebugStag",
    "ios-production-debug": "react-native run-ios --scheme RNBProd --configuration DebugProd",
    "ios-dev-release": "react-native run-ios --scheme RNBDev --configuration ReleaseDev",
    "ios-staging-release": "react-native run-ios --scheme RNBStag --configuration ReleaseStag",
    "ios-production-release": "react-native run-ios --scheme RNBProd --configuration ReleaseProd",
    "pod-install": "npx pod-install ios",
    "ios-build": "xcodebuild -workspace ios/RNB.xcworkspace -scheme RNBDev build CODE_SIGNING_ALLOWED=NO",
    "ios-clean": "cd ios && rm -rf build && cd ..",
    "android-ios": "yarn android && yarn ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "deeplink-android": "npx uri-scheme open 'rnapp://user/1' --android",
    "deeplink-ios": "npx uri-scheme open 'rnapp:user/2' --ios",
    "detox-build-ios-debug": "detox build -c ios.dev.debug",
    "detox-build-ios-release": "detox build -c ios.dev.release",
    "detox-test-ios-debug": "detox test -c ios.dev.debug",
    "detox-test-ios-release": "detox test -c ios.dev.release",
    "detox-build-android-debug": "detox build -c android.dev.debug",
    "detox-build-android-release": "detox build -c android.dev.release",
    "detox-test-android-debug": "detox test -c android.dev.debug",
    "detox-test-android-release": "detox test -c android.dev.release",
    "e2e-ios": "yarn detox-build-ios-debug && yarn detox-test-ios-debug",
    "e2e-ios-release": "yarn detox-build-ios-release && yarn detox-test-ios-release",
    "e2e-android": "yarn detox-build-android-debug && yarn detox-test-android-debug",
    "e2e-android-release": "yarn detox-build-android-release && yarn detox-test-android-release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install",
    "asset-link": "npx react-native-asset",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "np": "np --no-publish",
    "postversion": "react-native-version"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.8",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "@reduxjs/toolkit": "^1.8.5",
    "i18next": "^21.9.2",
    "react": "18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.36.1",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.2",
    "react-native-bootsplash": "^4.3.2",
    "react-native-dotenv": "^3.3.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-gesture-handler": "^2.7.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mask-input": "^1.2.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.0",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-webview": "^11.23.1",
    "react-redux": "^8.0.4",
    "redux-persist": "^6.0.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-named-capturing-groups-regex": "^7.19.1",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.1.0",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^5.0.0",
    "@testing-library/react-native": "^11.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.1.1",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^19.12.5",
    "eslint": "^8.24.0",
    "eslint-plugin-detox": "^1.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "metro-react-native-babel-preset": "^0.72.3",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "react-dom": "18.0.0",
    "react-native-rename": "^2.9.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  }
}
