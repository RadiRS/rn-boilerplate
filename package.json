{
  "name": "rnboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "android-build": "yarn android-clean; cd android && ./gradlew app:assembleRelease && cd ..",
    "android-build-bundle": "yarn android-clean; cd android && ./gradlew app:bundleRelease && cd ..",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios-clean": "cd ios && rm -rf build && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "set-env-dev": "cp src/config/environment/develop.env .env",
    "set-env-test": "cp src/config/environment/testing.env .env",
    "set-env-prod": "cp src/config/environment/production.env .env",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.65.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "husky": "^7.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
